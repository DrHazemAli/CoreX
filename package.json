{
  "name": "corex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:webpack": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint . --fix --max-warnings=2000",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "security": "npm audit --audit-level=moderate",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "remove:samples": "node scripts/remove-samples.js",
    "build:all-ts-errors": "__NEXT_TEST_MODE=true next build",
    "analyze": "npx next experimental-analyze"
  },
